{
  "name": "naist-bus-schedule",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "ava": "^0.25.0",
    "flow-bin": "^0.65.0",
    "gh-pages": "^1.1.0",
    "mz": "^2.7.0",
    "prettier": "^1.10.2",
    "react-scripts": "1.1.1",
    "selenium-standalone": "^6.12.0",
    "source-map-explorer": "^1.5.0",
    "string.prototype.padstart": "^3.0.0",
    "webdriverio": "^4.10.2"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "holiday_jp": "holiday-jp/holiday_jp-js",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
